type Query {
    itemById(id: ID): ItemType
    itemByItemProperty(id: ID, name: String): ItemType
    getByItem(item : ItemInput): [ItemType]
}

type ItemType {
    id: ID
    name: String
    type: Type
    itemProperty: ItemProperty
}

input ItemInput {
    id: ID
    name: String
    type: Type
}

enum Type {
    ALPHA
    BETA
    OMEGA
}

type ItemProperty {
    id: Int
    name: String
}